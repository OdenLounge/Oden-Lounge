{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://oden-lounge-backend.vercel.app/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
